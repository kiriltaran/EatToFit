{
  "name": "eattofit",
  "description": "EatToFit: fitness application",
  "version": "1.0.0",
  "author": "kiriltaran <kiriltaran@gmail.com>",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["html"]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "element-ui": "^2.0.8",
    "firebase": "^4.8.0",
    "vue": "^2.5.11",
    "vue-resource": "^1.3.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-vue-app": "^2.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "file-loader": "^1.1.6",
    "node-sass": "^4.7.2",
    "prettier": "^1.9.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "vue-loader": "^13.6.0",
    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
